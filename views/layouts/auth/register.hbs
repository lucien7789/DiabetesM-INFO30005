<section id="form">
    <a id="back-link" href="/auth/login">Have an account already? Log in</a>
    <div class = "form-box">
        <form class = "flex-column" onsubmit="onSubmitPlaceholder(event)" method="post">
            <!-- Needs work. Not sure how to send username and password. 
            Will also need to redirect to home screen after login or password reset page -->
            
            <input id="username-input" type="text" name="username" placeholder="Email">
            <input id="first-name-input" type="text" name="first-name" placeholder="First Name">
            <input id="last-name-input" type="text" name="last-name" placeholder="Last Name">
            <input id="password-input" type="password" name="password" placeholder="Password">
            <input id="password-confirm-input" type="password" name="password-confirm" placeholder="Confirm Password">
            <br>
            <button class="button-standard button-standard-medium" type="submit" onclick="onSubmitClinician()" name="">Sign up as clinician</button>
        </form>
    </div>
</section>

<script src="/scripts/auth/registerForm.js"></script>